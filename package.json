    "babel-plugin-transform-class-properties": "^6.24",
    "babel-plugin-transform-decorators": "^6.24",
    "babel-plugin-transform-decorators-legacy": "^1.3",
    "babel-preset-env": "^1.7",
    "babel-preset-react": "^6.24",
    "eslint": "^5.8.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.11.1",
    "node-sass": "^4.9.4",
    "parcel-bundler": "^1.10",
    "babel-eslint": "^10.0.1"
  },
  "dependencies": {
    "d3": "^5.7",
    "lodash": "^4.17",
    "mobx": "^5.5",
    "mobx-react": "^5.3",
    "mobx-utils": "^5.0",
    "react": "^16.5",
    "react-dom": "^16.5",
    "react-hot-loader": "^4.3"
  }
